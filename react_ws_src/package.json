{
  "name": "XTTT-react-client",
  "description": "X Tic Tac Toe game demo front end built with React.js stack",
  "version": "0.0.1",
  "author": "Maxim Shklyar, kisla interactive, <maxim@kisla.com>",
  "private": true,
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/xims/X-TTT/react_ws_src"
  },
  "scripts": {
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "jest --coverage",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:webpack_dev",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:webpack_dev": "cross-env NODE_ENV=development webpack --config webpack.config.prod.js",
    "bc": "npm run build && npm run build:copyall_win",
    "bu": "npm run build && npm run build:copyupd_win",
    "bv": "npm version patch",
    "build:copyall_win": "xcopy dist\\* ..\\WS\\public /E /Y /D && xcopy static\\* ..\\WS\\public /E /Y /D",
    "build:copyupd_win": "copy dist\\bundle.js ..\\WS\\public\\ && copy dist\\style.css ..\\WS\\public\\",
    "build:copyall_win_mac": "npm run build && cp -r dist/* ../WS/public/ && cp -r static/* ../WS/public/",
    "build:mac": "npm run build && mkdir -p ../WS/public && cp -r dist/* ../WS/public/ && cp -r static/* ../WS/public/",
    "deploy:mac": "NODE_ENV=development npm run build:mac && cd ../WS && npm install && npm start"
  },
  "dependencies": {
    "ampersand-app": "^2.0.0",
    "framer-motion": "^10.18.0",
    "get-size": "^2.0.3",
    "gsap": "^3.12.5",
    "jquery": "^3.7.1",
    "object-assign": "^4.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-ga": "^3.3.1",
    "react-image": "^4.1.0",
    "react-motion": "^0.5.2",
    "react-router-dom": "^6.22.2",
    "socket.io-client": "^4.7.4",
    "superagent": "^9.0.1",
    "validator": "^13.11.0",
    "x2js": "^3.4.4",
    "xml-loader": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "express": "^4.18.3",
    "express-http-proxy": "^2.0.0",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "rimraf": "^5.0.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.0.0",
    "webpack-hot-middleware": "^2.26.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
